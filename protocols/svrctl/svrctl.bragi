namespace "managarm::svrctl"

enum Error {
	SUCCESS = 0,
	ILLEGAL_REQUEST = 1,
	// For CTL_BIND: The driver cannot bind to this device.
	DEVICE_NOT_SUPPORTED = 2,
	// For the first response to FILE_UPLOAD: Client needs to send data.
	DATA_REQUIRED = 3
}

enum CntReqType {
	NONE = 0,
	FILE_UPLOAD = 1,
	SVR_RUN = 2,
	FILE_UPLOAD_DATA = 3,

	PASSTHROUGH = 4
}
